#20231013
echo "#!/bin/ash
exec urxvt -bg '#f0f0f0' -fg black -name iotop -fn 'xft:Roboto Mono:style=Regular:pixelsize=14' -fb 'xft:Roboto Mono:style=Bold:pixelsize=14' -e ${IOTOPEXE} -o -a
" > usr/bin/iotop-wrapper
chmod 755 usr/bin/iotop-wrapper
